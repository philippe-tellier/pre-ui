{
    "name": "pre-ui",
    "version": "0.2.0",
    "description": "Extendable UI component library using Vue.js and Tailwind CSS.",
    "author": "Philippe Tellier <dev@philippetellier.com>",
    "license": "MIT",
    "repository": "https://github.com/philippe-tellier/pre-ui.git",
    "bugs": "https://github.com/philippe-tellier/pre-ui/issues",
    "homepage": "https://github.com/philippe-tellier/pre-ui",
    "main": "dist/pre-ui.ssr.js",
    "browser": "dist/pre-ui.esm.js",
    "module": "dist/pre-ui.esm.js",
    "unpkg": "dist/pre-ui.min.js",
    "files": [
        "dist/*",
        "src/**/*.vue"
    ],
    "sideEffects": false,
    "scripts": {
        "serve": "npm run storybook",
        "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
        "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
        "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
        "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "dependencies": {
        "insert-text-at-cursor": "^0.3.0",
        "v-money": "^0.8.1",
        "vue-the-mask": "^0.11.1"
    },
    "devDependencies": {
        "@babel/core": "^7.12.17",
        "@babel/preset-env": "^7.12.11",
        "@nuxtjs/tailwindcss": "^3.4.2",
        "@rollup/plugin-alias": "^3.1.1",
        "@rollup/plugin-babel": "^5.2.2",
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-node-resolve": "^11.0.1",
        "@rollup/plugin-replace": "^2.3.4",
        "@storybook/addon-actions": "^6.1.18",
        "@storybook/addon-essentials": "^6.1.18",
        "@storybook/addon-links": "^6.1.18",
        "@storybook/vue": "^6.1.18",
        "@tailwindcss/postcss7-compat": "^2.0.3",
        "@vue/cli-plugin-babel": "^4.5.11",
        "@vue/cli-service": "^4.5.11",
        "autoprefixer": "^9.8.6",
        "babel-eslint": "^10.1.0",
        "cross-env": "^7.0.3",
        "eslint": "^7.5.0",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-vue": "^6.2.2",
        "flatpickr": "^4.6.3",
        "minimist": "^1.2.5",
        "postcss": "^7.0.35",
        "rollup": "^2.36.1",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-vue": "^5.1.9",
        "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.3",
        "vue": "^2.6.12",
        "vue-template-compiler": "^2.6.12"
    },
    "peerDependencies": {
        "flatpickr": "^4.6",
        "tailwindcss": "^2.0",
        "vue": "^2.6"
    },
    "engines": {
        "node": ">=12.13"
    }
}
